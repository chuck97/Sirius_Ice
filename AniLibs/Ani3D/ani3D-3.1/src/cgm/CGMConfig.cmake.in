set(CGM_FOUND TRUE)

set(CGM_INCLUDE_DIRS "@CMAKE_INSTALL_PREFIX@/include")

set(CGM_HAS_PARALLEL "@CGM_PARALLEL@")

set(CGM_HAS_CUBIT "@CUBIT_FOUND@")
if (CGM_HAS_CUBIT)
  set(old_cmake_module_path "${CMAKE_MODULE_PATH}")
  list(INSERT CMAKE_MODULE_PATH 0
    "${CMAKE_CURRENT_LIST_DIR}")
  if (NOT CUBITROOT)
    set(CUBITROOT "@CUBITROOT@")
  endif ()
  find_package(CUBIT REQUIRED)
  set(CMAKE_MODULE_PATH "${old_cmake_module_path}")
  unset(old_cmake_module_path)
endif ()

set(CGM_HAS_OCC "@CGM_OCC@")

include("${CMAKE_CURRENT_LIST_DIR}/CGMTargets.cmake")

set(CGM_LIBRARIES
  cgm
  cgma_init)
